<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/GreySmoke"
    android:orientation="vertical">


    <include
        android:id="@+id/fragment3Top"
        layout="@layout/base_top"
        android:layout_width="match_parent"
        android:layout_height="70dp"></include>


    <com.example.administrator.ui_sdk.View.MyWebView
        android:id="@+id/fragment3WebView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/fragment3Top" />

    <com.example.administrator.ui_sdk.View.CircleWaiting
        android:id="@+id/fragment3Wait"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/fragment3Top" />

</RelativeLayout>