<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/airkissRelative"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <com.example.administrator.ui_sdk.View.CircleLoading
        android:id="@+id/wifiConn"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


    <EditText
        android:id="@+id/wifiSSID"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/edit2"
        android:hint="@string/wifiNameHint"
        android:padding="5dp"
        android:textColor="@color/Black"
        android:textSize="15sp" />


    <EditText
        android:id="@+id/wifiPassword"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/wifiSSID"
        android:layout_marginTop="5dp"
        android:background="@drawable/edit2"
        android:hint="@string/wifiPasswordHint"
        android:padding="5dp"
        android:textColor="@color/Black"
        android:textSize="15sp" />


